<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Ubicacion.
    -->
    <changeSet id="20190919193407-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="provincia_id"
                                 baseTableName="ubicacion"
                                 constraintName="fk_ubicacion_provincia_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provincia"/>

        <addForeignKeyConstraint baseColumnNames="nombre_ubicacion_id"
                                 baseTableName="ubicacion"
                                 constraintName="fk_ubicacion_nombre_ubicacion_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

    </changeSet>
</databaseChangeLog>
